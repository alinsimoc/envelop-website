{
  "name": "@envelop/sentry",
  "version": "0.2.1",
  "author": "Dotan Simha <dotansimha@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/envelop.git",
    "directory": "packages/plugins/sentry"
  },
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "typings": "lib/index.d.ts",
  "typescript": {
    "definition": "lib/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./*": {
      "require": "./*.js",
      "import": "./*.mjs"
    }
  },
  "scripts": {
    "test": "jest",
    "prepack": "bob-esbuild build"
  },
  "devDependencies": {
    "@sentry/node": "6.4.1",
    "@sentry/tracing": "6.4.1",
    "bob-esbuild-cli": "^0.1.13",
    "graphql": "15.5.0",
    "typescript": "4.3.2"
  },
  "peerDependencies": {
    "@sentry/node": "^6",
    "graphql": "^14.0.0 || ^15.0.0"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  }
}
